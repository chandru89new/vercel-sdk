{
  "name": "vercel-sdk",
  "version": "1.0.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "dependencies": {
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "parcel": "^2.7.0",
    "sanctuary": "^3.1.0",
    "typescript": "^4.8.3"
  },
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "scripts": {
    "dev": "yarn ts-node",
    "typecheck": "yarn tsc -p tsconfig.test.json --noEmit",
    "build": "yarn clean && yarn generate-defs && yarn parcel-build",
    "clean": "rm -rf dist",
    "generate-defs": "yarn tsc -p tsconfig.json",
    "parcel-build": "yarn parcel build ./src/index.ts",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.18",
    "@types/node-fetch": "^2.6.2",
    "@types/sanctuary": "^3.1.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.0.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "url": "^0.11.0",
    "util": "^0.12.4"
  },
  "files": [
    "dist",
    "readme.md"
  ]
}
